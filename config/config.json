{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "gestion-documental-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/gestionDocumental/GestionDocumentalWebPart.js",
          "manifest": "./src/webparts/gestionDocumental/GestionDocumentalWebPart.manifest.json"
        }
      ]
    },
    "busqueda-documentos-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/busquedaDocumentos/BusquedaDocumentosWebPart.js",
          "manifest": "./src/webparts/busquedaDocumentos/BusquedaDocumentosWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},  
  "localizedResources": {
    "GestionDocumentalWebPartStrings": "lib/webparts/gestionDocumental/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "BusquedaDocumentosWebPartStrings": "lib/webparts/busquedaDocumentos/loc/{locale}.js"
  }
}
